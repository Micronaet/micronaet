<?xml version="1.0"?>
<openerp>
<data noupdate="1">
    <record id="aeroo_order_appointment_operation" model="ir.actions.report.xml">
        <field name="name">Operation appointment per order</field>
        <field name="type">ir.actions.report.xml</field>
        <field name="model">sale.order</field>
        <field name="report_name">dentist_order_appointment_operation_report</field>
        <field name="report_type">aeroo</field>
        <field name="in_format">oo-odt</field>
        <field name="parser_loc">dentist/report/parser_order_appointment.py</field>
        <field name="report_rml">dentist/report/order_appointment.odt</field>
        <field name="parser_state">loc</field>
        <field name="tml_source">file</field>
        <field name="out_format" eval="ref('report_aeroo_ooo.report_mimetypes_pdf_odt')"></field>
    </record>
    
    <ir_set>
        <field eval="'action'" name="key"/>
        <field eval="'client_print_multi'" name="key2"/>
        <field eval="['sale.order']" name="models"/>
        <field name="name">dentist_appointment_operation_report</field>
        <field eval="'ir.actions.report.xml,'+str(aeroo_order_appointment_operation)" name="value"/>
        <field eval="True" name="isobject"/>
        <field eval="True" name="replace"/>
    </ir_set>
</data>
</openerp>

