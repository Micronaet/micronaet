<?xml version="1.0"?>
<openerp>
    <data noupdate="1">
        <record id="aeroo_mrp_line_setup_id" model="ir.actions.report.xml">
            <field name="name">Setup line</field>
            <field name="type">ir.actions.report.xml</field>
            <field name="model">mrp.workcenter</field>
            <field name="report_name">aeroo_workcenter_line_setup_report</field>
            <field name="report_type">aeroo</field>
            <field name="in_format">oo-odt</field>
            <field name="out_format" eval="ref('report_aeroo_ooo.report_mimetypes_pdf_odt')" />
            <field name="parser_loc">production_line_setup/report/setup_parser.py</field>
            <field name="report_rml">production_line_setup/report/setup.odt</field>
            <field name="parser_state">loc</field>
            <field name="tml_source">file</field>
        </record>

        <ir_set>
            <field eval="'action'" name="key"/>
            <field eval="'client_print_multi'" name="key2"/>
            <field eval="['mrp.workcenter']" name="models"/>
            <field name="name">aeroo_workcenter_line_setup_report_set</field>
            <field eval="'ir.actions.report.xml,'+str(aeroo_mrp_line_setup_id)" name="value"/>
            <field eval="True" name="isobject"/>
            <field eval="True" name="replace"/>
        </ir_set>
    </data>
</openerp>

