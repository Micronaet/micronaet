        <field name="search_view_id" ref="view_support_issue_case_search"/>
        <field name="domain">[('is_case','=',True)]</field>
        <field name="context">{'form_view_ref':'technical_support.view_support_issue_case_form'}</field>
    </record>

    <!--ACT_WINDOW-->
      <act_window
            id="act_support_issue_comunication"
            name="List: Comunications"
            src_model="support.issue"
            res_model="support.issue.comunication"
            view_type="form"
            view_mode="tree,form"
            domain="[('issue_id','=',active_id)]"
            context="{'default_issue_id':active_id}"
            />
            
      <act_window
            id="act_support_intervention_report"
            name="List: Intervents"
            src_model="support.issue"
            res_model="intervention.report"
            view_type="form"
            view_mode="tree,form"
            domain="[('issue_ids','=',[active_id])]"            
            context="{'default_partner_id':partner_id}"
            /><!--TODO Maybe not works on mixed intervent: 1 intervent that have 2 issue-->

         <!--4 type of intervent:-->
      <act_window
            id="act_support_issue_new_intervent"
            name="New intervention: customer"
            src_model="support.issue"
            res_model="intervention.report"
            view_type="form"
            view_mode="form"            
            context="{'default_partner_id':partner_id,'default_issue_ids':[active_id],'default_name':name,'default_intervention_request':description,'default_date_start':time.strftime('%Y-%m-%d %H:00:00'),'default_mode':'customer'}"
            />
      <act_window
            id="act_support_issue_new_company"
            name="New intervention: company"
            src_model="support.issue"
            res_model="intervention.report"
            view_type="form"
            view_mode="form"            
            context="{'default_partner_id':partner_id,'default_issue_ids':[active_id],'default_name':name,'default_intervention_request':description,'default_date_start':time.strftime('%Y-%m-%d %H:00:00'),'default_mode':'company'}"
            />
      <act_window
            id="act_support_issue_new_connection"
            name="New intervention: connection"
            src_model="support.issue"
            res_model="intervention.report"
            view_type="form"
            view_mode="form"            
            context="{'default_partner_id':partner_id,'default_issue_ids':[active_id],'default_name':name,'default_intervention_request':description,'default_date_start':time.strftime('%Y-%m-%d %H:%M:00'),'default_mode':'connection'}"
            />
      <act_window
